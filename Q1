#include<stdio.h>
#include<conio.h>
#include<stdlib.h>
#define size 5000
int queue[size],front= -1,rear= -1;
void enqueue(int);
void display();
void calc();
void main()
{
int i,n;
int value;
clrscr();
printf("no of elements to insert:");
scanf("%d",&n);
for(i=0;i<n;i++)
{
scanf("%d",&value);
enqueue(value);
}
display();
calc();
}
void enqueue(int value)
{
if(rear==size-1)
{
printf("queue--full");
}
else
{
if(front==-1)
front=0;
rear++;
queue[rear]=value;
}
}
